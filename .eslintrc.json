{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "indent": ["warn", 4],
    "quotes": ["warn", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": ["warn"],
    "eqeqeq": ["error", "always"],
    "curly": ["error"],
    "camelcase": ["error"],
    "no-trailing-spaces": ["error"],
    "eol-last": ["error", "always"],
    "no-multiple-empty-lines": ["error"]
  }
}
